{
  "name": "Standard Guitar Tuner",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "description": "Extension for tuning a guitar to EADGBe standard tuning.",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "assets/headstock.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
