{
  "name": "Guitar Tuner",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "description": "Extension for tuning instruments",
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["assets/*.wav"],
      "matches": ["<all_urls>"]
    }
  ]
}
